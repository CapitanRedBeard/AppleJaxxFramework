{
  "pages": [
    {
      "key": "list_page",
      "style": {
          "backgroundColor": "#4D5057"
      },
      "components": [
        {
          "type": "list",
          "style": {
            "backgroundColor": "#999999"
          },
          "dataSource": {
            "url": "https://facebook.github.io/react-native/movies.json"
          },
          "rowDataBinding": "movies",
          "rowOnClickEval": {
            "key": "feature_page",
            "type": "push",
            "param": "movies"
          },
          "rowComponents": [
            {
              "type": "text",
              "style": {
                "fontSize": 20,
                "color": "white"
              },
              "binding": "title"
            },
            {
              "type": "text",
              "style": {
                "fontSize": 16,
                "color": "#CCCCCC"
              },
              "binding": "releaseYear"
            }
          ],
          "separator": {},
          "header": {},
          "footer": {},
          "sectionHeader": {}
        }
      ]
    }
  ],
  "metaData": {
    "frameID": "listFrame"
  }
}
