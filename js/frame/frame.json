{
  "drawer": {
     "left": {
         "screen": "drawer"
     },
     "right": {
         "screen": "drawer"
     },
     "animationType": "door",
     "style": {
         "leftDrawerWidth": 50,
         "rightDrawerWidth": 100,
         "drawerShadow": false,
         "contentOverlayColor": "#333333"
     }
  },
  "pages": [
    {
      "key": "page1",
      "title": "Page One",
      "subtitle": "numero uno",
      "navigatorStyle": {
        	"navBarBackgroundColor": "#4e8ef7",
        	"drawUnderNavBar": false,
        	"navBarTextColor": "white",
        	"navBarButtonColor": "white",
        	"statusBarTextColorScheme": "light"
      },
      "navigatorButtons": {
        "rightButtons": [
          {
            "title": "Edit",
            "id": "edit",
            "disabled": true,
            "disableIconTint": true,
            "showAsAction": "ifRoom",
          },
          {
            "icon": "ios-ionic",
            "id": "menu2",
            "event": {
              "eventType": "toggleDrawer",
              "params": {
                "side": "right",
                "animated": true,
              }
            }
          }
        ],
        "leftButtons": [
          {
            "icon": "ios-menu",
            "id": "menu",
            "event": {
              "eventType": "toggleDrawer",
              "params": {
                "side": "left",
                "animated": true,
              }
            }
          }
        ],
        "animated": true
      },
      "style": {
        "backgroundColor": "#dddddd",
      },
      "components": [
        {
          "type": "list",
          "style": {
            "backgroundColor": "white"
          },
          "refreshable": {
              "tintColor": "#4e8ef7",
              "title": "Loading...",
              "titleColor": "#4e8ef7"
          },
          "rowData": {
            "type": "url",
            "params": {
              "url": "https://jsonplaceholder.typicode.com/users/",
            }
          },
          "rowTemplate": {
            "leftSection": [
              {
                 "type": "thumbnail",
                 "source": "https://unsplash.it/200/300/?random",
                 "attributes": {
                   "square": true,
                   "size": 50
                 }
              }
            ],
            "centerSection": [
              {
                "type": "text",
                "style": {
                  "fontSize": 16,
                  "color": "#333",
                  "paddingLeft": 10
                },
                "text": "{{name}}"
              },
              {
                "type": "text",
                "style": {
                  "fontSize": 12,
                  "lineHeight": 12,
                  "color": "#888",
                  "paddingLeft": 20
                },
                "text": "Near {{address.city}}..."
              }
            ],
            "rightSection": [
              {
                "type": "icon",
                "style": {
                  "color": "#4e8ef7"
                },
                "name": "ios-swap-outline"
              }
            ]
          },
          "separator": {
              "backgroundColor": "#4e8ef7",
              "marginLeft": 75
          }
        }
      ]
    },
    {
      "key": "page2",
      "title": "Page Two",
      "subtitle": "numero duo",
      "navigatorStyle": {
        	"navBarBackgroundColor": "#292949",
        	"navBarTextColor": "#EDECEE",
        	"navBarButtonColor": "#EDECEE",
        	"statusBarTextColorScheme": "light"
      },
      "navigatorButtons": {
        "leftButtons": [
          {
            "icon": "ios-menu",
            "id": "menu",
            "event": {
              "eventType": "toggleDrawer",
              "params": {
                "side": "left",
                "animated": true,
