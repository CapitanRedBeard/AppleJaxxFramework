              "dataSource": {
                "type": "param_url",
                "url": "pokemonz.url"
              }
            }
          },
          "rowData": {
            "type": "raw",
            "params": {
              "data": [
                {
                  "list_thumbnail_source": "https://maxcdn.icons8.com/Color/PNG/96/Plants/broccoli-96.png",
