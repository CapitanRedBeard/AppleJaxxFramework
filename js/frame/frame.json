    "tabs": [
      {
        "label": "Friends",
        "icon": "ios-person-add-outline",
        "screen": "page1",
      },
      {
        "label": "Taste",
        "icon": "ios-list-box-outline",
        "screen": "page2",
      },
      {
        "label": "Folders",
        "icon": "ios-folder-open-outline",
        "screen": "page3"
      }
